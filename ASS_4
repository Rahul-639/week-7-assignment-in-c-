#include <stdio.h>
#define MOD 1000000007
void Series(int n) {
    long long fib[n + 1];
    fib[0] = 0;
    if (n >= 1) fib[1] = 1;
    for (int i = 2; i <= n; i++) {
        fib[i] = (fib[i - 1] + fib[i - 2]) % MOD;
    }
    for (int i = 0; i <= n; i++) {
        printf("%lld ", fib[i]);
    }
}
int main() {
    int n;
    scanf("%d", &n);   
    Serie(n);         
    return 0;
}
