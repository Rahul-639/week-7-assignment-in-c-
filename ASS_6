#include <stdio.h>
#include <string.h>
#include <stdbool.h>
bool isPalindrome(char s[]) {
    int left = 0;
    int right = strlen(s) - 1;
    while (left < right) {
        if (s[left] != s[right]) {
            return false;   // mismatch found
        }
        left++;
        right--;
    }
    return true;  // all matched
}
int main() {
    char s[1000006];  
    scanf("%s", s);    
    if (isPalindrome(s))
        printf("true\n");
    else
        printf("false\n");
    return 0;
}
